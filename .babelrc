{
  "presets": [
    "react",
    "es2015",
    "stage-1"
  ],
  "plugins": [
    "transform-function-bind",
    "transform-decorators-legacy",
    "transform-class-properties",
    "px2rem"
  ],
  "env": {
    "test": {
      "plugins": [
        ["transform-remove-console"],
        [ "babel-plugin-webpack-alias", { "config": "./internals/webpack/webpack-test-config.js" } ]
      ]
    }
  }
}
